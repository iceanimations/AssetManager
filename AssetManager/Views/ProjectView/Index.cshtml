@model AssetManager.Models.Project
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    ViewBag.Title = Model.Name;
}

<h2>@Model.Name</h2>
<div class="row">
    <div class="col-md-4">
        @foreach (var cat in Model.Categories)
        {
            foreach (var asset in cat.Assets)
            {
                <h4>@asset.Name</h4>
                <p>@cat.Name</p>
                <br />
                
            }
        }
    </div>
</div>