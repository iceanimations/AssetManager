@model IEnumerable<AssetManager.Models.ProjectType>

@{
    ViewBag.Title = "Project Types";
}

<script>
    $("#locBar").append("<span>Project Types</span>");
</script>

<h2>Project Types</h2>

<p>
    @Html.ActionLink("Create New Type", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LocationUNC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LocationDisplay)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LocationUNC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LocationDisplay)
        </td>
        <!--
        <td>
            @@Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @@Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @@Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        -->
    </tr>
}

</table>
